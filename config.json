    "sections": [
      {
        "name": "Prematch",
        "fields": [
          {
            "title": "Scouter Initials",
            "description": "Enter the initials of the scouter.",
            "type": "text",
            "required": true,
            "code": "scouter",
            "formResetBehavior": "preserve",
            "defaultValue": ""
          },
          {
            "title": "Match Number",
            "description": "Select the match number.",
            "type": "number",
            "required": true,
            "code": "matchNumber",
            "formResetBehavior": "reset",
            "defaultValue": 1
          },
          {
            "title": "Team and Robot",
            "description": "Select the team and robot position you are scouting.",
            "type": "number",
            "required": true,
            "code": "robot",
            "formResetBehavior": "reset",
            "defaultValue": 0
          },
          {
            "title": "Starting Position",
            "description": "The starting position of the robot.",
            "type": "select",
            "required": true,
            "code": "startPos",
            "formResetBehavior": "reset",
            "defaultValue": "",
            "choices": {
              "OT": "Outpost Trench",
              "OBFT": "Outpost Bump Favoring Trench",
              "OBFH": "Outpost Bump Favoring Hub",
              "H": "Hub",
              "DBFH": "Depot Bump Favoring Hub",
              "DBFT": "Depot Bump Favoring Trench",
              "DT": "Depot Trench"
            }
          },
          {
            "title": "No Show",
            "description": "Check if the robot did not show up for the match.",
            "type": "boolean",
            "required": false,
            "code": "noShow",
            "formResetBehavior": "reset",
            "defaultValue": false
          }
        ]
      },
      {
