          },
          {
            "title": "Where Climbed",
            "description": "Where on the tower did the robot climb in autonomous?",
            "type": "select",
            "required": false,
            "code": "autoClimbPos",
            "formResetBehavior": "reset",
            "defaultValue": "No",
            "choices": {
              "No": "No Climb",
              "Out": "Output Side",
              "Mid": "Middle",
              "Dep": "Depot Side"
            }
          }
        ]
      },
